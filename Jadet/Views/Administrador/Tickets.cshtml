@model Jadet.Models.listaTicketsmodel

@{
    ViewBag.Title = "Clientes";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<h2>@ViewBag.Title</h2>
<table class="table table-active table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nota</th>
            <th>Ticket</th>
            <th>Fecha</th>
            <th>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.Items)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.IdNota</td>
            <td><img src="~/Content/tickets/@item.Ruta" width="100"/></td>
            <td>@item.Fecha</td>
            <td>
                
            </td>
        </tr>
        }
    </tbody>
</table>
